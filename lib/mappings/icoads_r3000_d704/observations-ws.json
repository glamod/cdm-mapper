{
    "observation_id": {
        "sections": "c98",
        "elements": "UID",
        "transform": "string_add",
        "kwargs":{"prepend":"ICOADS-30","append":"WS","separator":"-"}
    },
    "report_id": {
        "sections": "c98",
        "elements": "UID",
        "transform": "string_add",
        "kwargs":{"prepend":"ICOADS-30","separator":"-"}
    },
    "data_policy_licence": {
        "default": 0
    },
    "date_time": {
        "sections": ["c99_data4", "c99_data4", "c99_data4", "c99_data4", "c99_data4",
                     "c99_data5", "c99_data5", "c99_data5", "c99_data5", "c99_data5"],
        "elements": ["year", "month", "day", "hour", "time_ind",
                     "year", "month", "day", "hour", "time_ind"],
        "nan_filter": "any",
        "transform": "datetime_to_cdm_time"
    },
    "date_time_meaning": {
        "default": 2
    },
    "observation_duration": {
        "default": 8
    },
    "longitude": {
        "sections": "c99_daily",
        "elements": ["lon_deg_of", "lon_min_of", "lon_hemis_of"],
        "transform": "coord_dmh_to_180",
        "decimal_places": 2
    },
    "latitude": {
        "sections": "c99_daily",
        "elements": ["lat_deg_on", "lat_min_on", "lat_hemis_on"],
        "transform": "coord_dmh_to_90",
        "decimal_places": 2
    },
    "crs": {
        "default": 0
    },
    "z_coordinate_type": {
        "default": 0
    },
    "observed_variable": {
        "default": 107
    },
    "observation_value": {
        "sections": ["c99_data4", "c99_data5"],
        "elements": ["wind_force", "wind_force"],
        "nan_filter": "any",
        "transform": "get_wind_speed_from_table",
        "kwargs":{"scale":0}
    },
    "value_significance": {
        "default": 2
    },
    "units": {
        "default": 731
    },
    "conversion_flag": {
        "default": 2
    },
    "location_precision": {
        "sections": ["core", "c99_daily", "c99_daily", "c99_daily"],
        "elements": ["LI", "lat_deg_on","lat_min_on", "lat_hemis_on"],
        "transform": "location_accuracy",
        "decimal_places": "decimal_places"
    },
    "spatial_representativeness": {
        "default": 3
    },
    "quality_flag": {
        "default": 2
    },
    "numerical_precision": {
    },
    "sensor_automation_status": {
        "default": 5
    },
    "exposure_of_sensor": {
        "default": 3
    },
    "original_precision": {
    },
    "original_units": {
        "default": 731
    },
    "original_value": {
        "sections": ["c99_data4", "c99_data5"],
        "elements": ["wind_force", "wind_force"],
        "nan_filter": "any",
        "transform": "get_wind_speed_from_table",
        "kwargs":{"scale":0}
    },
    "processing_level": {
        "default": 3
    },
    "traceability": {
        "default": 2
    },
    "advanced_qc": {
        "default": 0
    },
    "advanced_uncertainty": {
        "default": 0
    },
    "advanced_homogenisation": {
        "default": 0
    },
    "source_id": {
        "sections":["c1","c1","core","core"],
        "elements": ["SID","DCK","YR","MO"],
        "transform": "string_join_add",
        "kwargs":{"prepend":"ICOADS-3-0-0T","separator":"-","zfill_col":[0,3],"zfill":[3,2]}
    }
}
