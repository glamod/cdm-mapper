{
    "report_id": {
        "sections": "c98",
        "elements": "UID",
        "transform": "string_add",
        "kwargs":{"prepend":"ICOADS-30","separator":"-"}
    },
    "application_area": {
        "default": [1,7,10,11]
    },
    "observing_programme": {
        "sections": "c1",
        "elements": "PT",
        "transform": "observing_programme"
    },
    "report_type": {
        "default": 0
