    "latitude": {
        "sections": "c99",
        "elements": "Lat",
        "decimal_places": "decimal_places"
    },
    "location_accuracy": {
        "sections": ["core","c99"],
        "elements": ["LI","Lat"],
        "transform": "location_accuracy"
    },
    "crs": {
        "default": 0
    },
    "station_speed": {
        "sections": "core",
        "elements": "VS"
    },
    "station_course": {
        "sections": "core",
        "elements": "DS"
    },
    "height_of_station_above_local_ground": {
        "default": 0
    },
    "height_of_station_above_sea_level": {
        "default": 0
    },
    "report_meaning_of_timestamp": {
        "default": 2
    },
    "report_timestamp": {
        "sections": "core",
        "elements": ["YR","MO","DY","HR"],
        "transform": "datetime_imma1"
    },
    "report_duration": {
        "default": 11
    },
    "report_time_accuracy": {
        "sections": "core",
        "elements": "TI",
