    "station_type": {
        "default": 2
    },
    "platform_type": {
        "sections": "c1",
        "elements": "PT",
        "fill_value": 2,
        "code_table": "platform_type"
    },
    "platform_sub_type": {
        "sections": "c1",
        "elements": "PT",
        "code_table": "platform_sub_type"
    },
    "primary_station_id": {
        "sections": "core",
        "elements": "ID"
    },
    "station_record_number": {
        "default": 1
    },
    "primary_station_id_scheme": {
        "sections": "c1",
        "elements": "PT",
        "fill_value": 5,
        "code_table": "primary_station_id_scheme"
    },
    "longitude": {
        "sections": "c99",
        "elements": "Lon",
        "transform": "float_opposite",
